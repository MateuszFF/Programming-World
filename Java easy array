public class Main {

    public static void main(String[] args) {

        int tab[] = new int[9];
        tab[0] = 1;
        tab[1] = 4;
        tab[2] = 5;
        tab[3] = 8;
        tab[4] = 3;
        tab[5] = 9;
        tab[6] = 5;
        tab[7] = 2;
        tab[8] = 8;
        for (int i : tab) {
            System.out.println("Element of the array = " + i );
        }

    }
}
